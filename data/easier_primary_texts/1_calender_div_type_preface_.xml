						<orig choice_type="oxford">Æglo<pc force="weak">-</pc><lb break="no" ed="1579"/>gaj</orig>
						<reg choice_type="char_norm default_archive">Æglo<pc force="weak">-</pc><lb break="no" ed="1579"/>gai</reg>
					</choice> as it <choice>
						<orig>vvere</orig>
						<reg choice_type="char_norm oxford default_archive">were</reg>
					</choice>
					<seg xml:lang="grc">
						<ref n="calender_864951082">
                        αἰγῶν
                        </ref><!-- 3N in scan check
